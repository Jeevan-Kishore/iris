{
  "name": "dsn",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": false
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "connector": {
      "type": "string",
      "required": true
    },
    "connectionParams": {
      "type": "object",
      "required": true
    },
    "displayName": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "userId": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    },
    "dsnDbs": {
      "type": "hasMany",
      "model": "dsnDb",
      "foreignKey": "dsnDbId"
    },
    "dsnTables": {
      "type": "hasMany",
      "model": "dsnTable",
      "foreignKey": "dsnTableId"
    },
    "dsnColumns": {
      "type": "hasMany",
      "model": "dsnColumn",
      "foreignKey": "dsnColumnId"
    }
  },
  "acls": [],
  "methods": {}
}
